<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="pingan.com.demo_4203.MainActivity">



   <LinearLayout
       android:layout_width="match_parent"
       android:layout_height="wrap_content"
       android:orientation="vertical"
       >

   <TextView
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:id="@+id/selectstand_layout04_text01"
       android:text="11"
       />
   <pingan.com.demo_4203.MyPopSeekBar
       android:id="@+id/selectstand_layout04_seekBar01"
       android:layout_width="255dip"
       android:layout_height="8dp"
       android:layout_marginLeft="20dip"
       android:layout_below="@+id/selectstand_layout04_text01"
       android:layout_marginTop="50dip"
       android:progressDrawable="@drawable/progressbar"
       android:secondaryProgress="1"
       android:thumb="@null" />
   </LinearLayout>
</RelativeLayout>
